#include<stdio.h>
#include<stdlib.h>

int main(){
    
  int i,n;
  printf("enter the number of integers ");
 scanf("%d",&n);
  
  int *ptr = (int*)malloc(n*sizeof(int));
  
  
  for(i=0;i<n;i++){
      printf("enter any intger ");
      scanf("%d",ptr+i);
      
  }
  for(i=0;i<n;i++){
      
      printf("%d",*(ptr+i));
  }
  
   return 0; 
}
